#! /usr/bin/env bash

parent_path=$( cd "$(dirname "${BASH_SOURCE[0]}")"; cd .. ;pwd -P )
cd $parent_path

DATASET="PMCC-HN-TEST"
LOCALISER="('localiser-pr','baseline','epoch=177-step=40227')"
NAME="Parotid_R-baseline"
REGION='Parotid_R'
SEGMENTER="('segmenter-pr','baseline','epoch=72-step=16497')"

python evaluate_model --name $NAME --dataset $DATASET --localiser $LOCALISER --segmenter $SEGMENTER --region $REGION
